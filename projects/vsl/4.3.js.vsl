function toggle${manageable.name}(inIndex, inShow) {
        			//if (!inShow) destroySubGridIdGeneral('',querydata${memberAs.association.name}[inIndex][1],"${manageable.name}MainDetail");
					if (!inShow) destroySubGridIdGeneral('',querydata${memberAs.association.name}[inIndex][1],"${manageable.name}Child");
				    detailRows${manageable.name}[inIndex] = inShow;
				    dijit.byId("grid${memberAs.association.name}").updateRow(inIndex); 
				    
    } 

function getCheck${manageable.name}(inRowIndex) {
	var image = (detailRows${manageable.name}[inRowIndex] ? 'open.gif' : 'closed.gif');
	var show = (detailRows${manageable.name}[inRowIndex] ? 'false' : 'true');
	return '<img src="../image/' + image + '" onclick="toggle${manageable.name}(' + inRowIndex + ', ' + show + ')" height="11" width="11">';
}

function getDetail${manageable.name}(inRowIndex){
	if(detailRows${manageable.name}[inRowIndex]){
		var cell = this;
		setTimeout(function() { buildSubgrid${memberAs.association.name}(inRowIndex, cell); }, 0);  }
		var h =0;
		if(detailRows${manageable.name}[inRowIndex]&&querydata${memberAs.association.name}[inRowIndex][$childRow].length>0){
							h=65+25*querydata${memberAs.association.name}[inRowIndex][$childRow].length;  		}
	return '<div style="height:'+h+'px; background-color: white;"></div>';
}

